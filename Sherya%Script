local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library//main/Library", true))()
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local window = library:AddWindow("HavShey VY5", {
    main_color = Color3.fromRGB(000, 000, 000),
    min_size = Vector2.new(660, 660),
    can_resize = false,
})

local mainTab = window:AddTab("Farm")
local statsTab = window:AddTab("Misc")
local eggsTab = window:AddTab("Farm")
local miscTab = window:AddTab("Killer")
local killTab = window:AddTab("Players")
local playerTab = window:AddTab("Credits")
